{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "consistent-return": 0,
    "comma-dangle": ["error", "always"],
    "no-use-before-define": 0,
    "func-names": 0,
    "max-len":1,
    "padded-blocks": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "semi": ["error", "never"],
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-bitwise": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "indent": [2, 2]
  },
  "plugins": [
    "import",
    "react"
  ]
}
